{
    "name": "svelte-tailwind-web-extension",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "run-p build:dev:*",
        "prod": "pnpm clean && run-p build:prod:*",
        "clean": "rimraf ./dist",
        "build:dev:pages": "vite build --mode development",
        "build:dev:content": "vite build -c vite.config.content.ts --mode development",
        "build:prod:pages": "vite build --mode production",
        "build:prod:content": "vite build -c vite.config.content.ts --mode production"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^2.0.2",
        "@tsconfig/svelte": "^3.0.0",
        "@types/chrome": "^0.0.204",
        "@types/webextension-polyfill": "^0.9.2",
        "autoprefixer": "^10.4.13",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.20",
        "prettier": "^2.8.1",
        "prettier-plugin-svelte": "^2.9.0",
        "prettier-plugin-tailwindcss": "^0.2.1",
        "rimraf": "^3.0.2",
        "svelte": "^3.55.0",
        "svelte-check": "^2.10.2",
        "svelte-preprocess": "^5.0.0",
        "tailwindcss": "^3.2.4",
        "tslib": "^2.4.1",
        "typescript": "^4.9.4",
        "vite": "^4.0.2",
        "vite-tsconfig-paths": "^4.0.3",
        "webextension-polyfill": "^0.10.0"
    },
    "dependencies": {
        "@material-design-icons/font": "^0.14.2"
    }
}
