{
    "name": "svelte-tailwind-web-extension",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "run-p build:dev:*",
        "prod": "pnpm clean && run-p build:prod:*",
        "clean": "rimraf ./dist",
        "build:dev:pages": "vite build --mode development",
        "build:dev:content": "vite build -c vite.config.content.ts --mode development",
        "build:prod:pages": "vite build --mode production",
        "build:prod:content": "vite build -c vite.config.content.ts --mode production"
    },
    "devDependencies": {
        "@iconify-json/mdi": "^1.1.50",
        "@sveltejs/vite-plugin-svelte": "^2.0.3",
        "@tsconfig/svelte": "^3.0.0",
        "@types/chrome": "^0.0.225",
        "@types/webextension-polyfill": "^0.10.0",
        "autoprefixer": "^10.4.14",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.21",
        "prettier": "^2.8.7",
        "prettier-plugin-svelte": "^2.10.0",
        "prettier-plugin-tailwindcss": "^0.2.5",
        "rimraf": "^4.4.1",
        "svelte": "^3.57.0",
        "svelte-check": "^3.1.4",
        "svelte-preprocess": "^5.0.3",
        "tailwindcss": "^3.2.7",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5",
        "unplugin-icons": "^0.15.3",
        "vite": "^4.2.1",
        "vite-tsconfig-paths": "^3.6.0",
        "webextension-polyfill": "^0.10.0"
    }
}
